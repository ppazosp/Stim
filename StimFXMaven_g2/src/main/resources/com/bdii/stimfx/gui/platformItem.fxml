<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="checkHbox" prefHeight="25.0" prefWidth="175.0" style="-fx-background-radius: 5; -fx-border-color: #1B2631; -fx-border-radius: 2; -fx-border-width: 5;" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bdii.stimfx.gui.PlatformItemController">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <styleClass>
      <String fx:value="primary-color" />
      <String fx:value="selected" />
   </styleClass>
   <children>
      <CheckBox fx:id="checkBox" focusTraversable="false" mnemonicParsing="false" onAction="#checkOnAction" prefHeight="18.0" prefWidth="155.0" stylesheets="@../css/styles.css" text="Platform" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></CheckBox>
   </children>
</HBox>
